{
    "name": "websecret/starter-kit",
    "description": "WebSecret starter kit",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1",
        "arcanedev/log-viewer": "^4.7",
        "cviebrock/eloquent-sluggable": "^4.8",
        "davejamesmiller/laravel-breadcrumbs": "^5.2",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.1",
        "jenssegers/date": "^3.5.0",
        "laravel/framework": "^5.8",
        "laravel/helpers": "^1.0",
        "laravel/nexmo-notification-channel": "^2.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8.0",
        "mcamara/laravel-localization": "^1.3",
        "predis/predis": "^1.1",
        "silber/bouncer": "v1.0.0-rc.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "^2.3",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/form.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
