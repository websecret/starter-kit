        "jquery-form": "^4.2.2",
        "jquery-pjax": "^2.0.1",
        "jquery-sortable": "^0.9.13",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "popper.js": "^1.15",
        "selectize": "^0.12.6",
